<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppThemeOrange" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorThemeOrange</item>
        <item name="colorPrimaryDark">@color/colorThemeOrange</item>
        <item name="colorAccent">@color/colorThemeOrange</item>
    </style>

    <style name="primary0">
        <item name="colorPrimary">@color/md_blue_500</item>
        <item name="colorPrimaryDark">@color/md_blue_700</item>
    </style>

    <style name="accent0">
        <item name="colorAccent">@color/md_blue_500</item>
    </style>

    <style name="primary1">
        <item name="colorPrimary">@color/md_green_500</item>
        <item name="colorPrimaryDark">@color/md_green_700</item>
    </style>

    <style name="accent1">
        <item name="colorAccent">@color/md_green_500</item>
    </style>


</resources>
